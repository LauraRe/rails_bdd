<% content_for :title, "New Article" %>

<h1>New Article</h1>

<%= form_for :article, url: articles_path do |form| %>
    <p>
        <%= form.label :title %><br>
        <%= form.text_field :title %>
    </p>
    <p>
        <%= form.label :content %><br>
        <%= form.text_area :content %>
    </p>
    <p>
        <%= form.submit button_text %>
    </p>
<% end %>
<%= link_to 'Back', articles_path %>